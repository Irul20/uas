
a= [1,4,8,9,3,6,4,6,7,3,5,7,0,5,3,7,8,4,7,4,12,13,45,72,12,98,54,1,34,67,2,4,5,6,7,3,13,14,15,16,17,15,76,54,53,67,34,35,67,3]
banyak_data=len(a)
print("data sebelum di urutkan:",a)

for i in range(banyak_data-1):
    for j in range(banyak_data-1):
        if a[j] > a[j+1]:
            temp = a[j]
            a[j] = a[j+1]
            a[j+1] = temp

print("data setelah di urutkan dengan bubble sort:",a)
print("banyak data:",banyak_data)
